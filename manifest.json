{
  "name": "NoMoreRestriction",
  "version": "0.1",
  "description": "Disable annoying restrictions on some pages.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "background_page": "background.html",

  "browser_action": {
    "default_title": "NoRestriction",
    "default_icon": "icon16.png",
    "popup": "popup.html"
  },

  "permissions": [
    "tabs", 
    "http://*/*",
    "https://*/*"
  ]
}

